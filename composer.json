{
  "name": "knot-lib/kernel",
  "description": "Kernel interfaces and objects for kNot Framework.",
  "keywords": ["PHP","PHP7","kNot Framework"],
  "homepage": "https://github.com/knot-lib/kernel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-php.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "psr/log": "^1.0",
    "psr/container": "^1.0",
    "psr/http-message": "~1.0",
    "psr/http-server-middleware": "^1.0",
    "nyholm/psr7": "^1.2",
    "stk2k/util": "^0.2",
    "stk2k/file-system": "^0.1",
    "knot-lib/exception": "^0.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.15",
    "php-coveralls/php-coveralls": "^2.0",
    "mikey179/vfsstream": "1.6.8",
    "phake/phake": "4.*"
  },
  "autoload": {
    "psr-4": {
      "knotlib\\kernel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "knotlib\\kernel\\test\\": "test/",
      "knotlib\\kernel\\test\\classes\\": "test/classes/"
    }
  }
}